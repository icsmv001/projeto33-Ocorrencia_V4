<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.cursojava.projetofinal.controller.CadastroSistemaController">
   <center>
      <AnchorPane prefHeight="99.0" prefWidth="229.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Sistema:" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" />
            <TextField fx:id="txtSistema" layoutX="14.0" layoutY="35.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" />
            <Button defaultButton="true" layoutX="14.0" layoutY="60.0" mnemonicParsing="false" onAction="#salvarOnAction" prefHeight="32.0" prefWidth="99.0" text="Salvar" />
            <Button cancelButton="true" layoutX="130.0" layoutY="60.0" mnemonicParsing="false" onAction="#limparOnAction" prefHeight="32.0" prefWidth="99.0" text="Limpar" />
         </children>
      </AnchorPane>
   </center>
   <bottom>
      <AnchorPane prefHeight="0.0" prefWidth="229.0">
         <children>
            <Label BorderPane.alignment="CENTER" />
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
